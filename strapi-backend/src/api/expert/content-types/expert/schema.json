{
  "kind": "collectionType",
  "collectionName": "experts",
  "info": {
    "singularName": "expert",
    "pluralName": "experts",
    "displayName": "Expert"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "original_quote": {
      "type": "integer"
    },
    "ra_comments": {
      "type": "text"
    },
    "bank_details": {
      "type": "text"
    },
    "expert_experiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience.experience",
      "mappedBy": "expert"
    },
    "companies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::company.company",
      "mappedBy": "experts"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "expert"
    },
    "source_of_response": {
      "type": "enumeration",
      "enum": [
        "Cold Call",
        "LinkedIn",
        "E-mail",
        "Reference",
        "Others"
      ]
    },
    "notes": {
      "type": "text"
    },
    "screening": {
      "type": "text"
    },
    "expert_status": {
      "type": "enumeration",
      "enum": [
        "Uncontacted",
        "No response",
        "Call again",
        "Contacted but not screened",
        "Contacted & screened",
        "Sent to client",
        "Negotiation",
        "Contacted but ghosting",
        "Six mos rule",
        "Out of budget",
        "NDA",
        "Not interested at all",
        "Not interested in project",
        "Call scheduled",
        "Call done"
      ]
    },
    "tags": {
      "type": "json"
    },
    "last_update": {
      "type": "component",
      "component": "update.update",
      "repeatable": false
    },
    "credits": {
      "type": "decimal"
    },
    "compliance": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    }
  }
}
